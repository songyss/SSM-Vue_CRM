<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csi.mapper.RolePermissionMapper">


    <select id="selectPermissions" resultType="com.csi.domain.RolePermission">
        select model_id,permissions_id
        from role_permission
        where role_id = #{roleId}
    </select>
    <select id="selectPermissionByList" resultType="string">
        select url
        from permissions
        where id = #{id}
    </select>


</mapper>